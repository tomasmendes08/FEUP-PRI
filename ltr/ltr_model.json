{
    "store" : "my_efi_feature_store",
    "name" : "my_efi_model",
    "class" : "org.apache.solr.ltr.model.LinearModel",
    "features" : [
    { 
      "name" : "maximize_score"
    },
    { 
      "name" : "maximize_votes"
    },
    { 
      "name" : "original_score",
      "norm" : {
        "class" : "org.apache.solr.ltr.norm.MinMaxNormalizer",
        "params" : { "min":"0", "max":"50"}
      }
    }
    ],
    "params" : {
      "weights" : {
        "maximize_score": 3.0,
        "maximize_votes": 3.0,
        "original_score" : 5.0
      }
    }
}